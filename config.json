{
  "tsr_model": {

    "training": {
      "tickers": ["GOOGL"],
      "start": "2022-01-01",
      "end": "2023-01-01",
      "seq_length": 24,
      "interval": "1d",
      "epochs": 40,
      "batch_size": 7,
      "lr": 0.0001
    },

    "testing": {
      "tickers": ["GOOGL"],
      "start": "2023-01-01",
      "end": "2024-01-01",
      "seq_length": 24,
      "interval": "1d",
      "epochs": 4,
      "batch_size": 1,
      "lr": 0.0001
    }
  },

  "sentiment_model": {
    "web_scraper": {
      "chains": [
        {
          "start_tag": "<small>",
          "end_tag": "</small>",
          "stock_name": "nvidia"
        }
      ],
      "output_name": "youtube_nvidia.raw"
    },
    "tokenization": {
      "tokenizer_model": "bert-base-uncased",
      "train_split": 0.8,
      "validation_split": 0.1,
      "batch_size": 16,
      "shuffle_data": true,
      "random_seed": 42
    }
  }
}